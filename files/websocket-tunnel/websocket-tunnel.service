[Unit]
Description=Websocket Tunnel
Requires=network.target
After=network.target

[Service]
Type=simple
EnvironmentFile=env
Environment=
WorkingDirectory={{ DIR.stdout }}
ExecStart=/usr/bin/env node server.js
PIDFile=/var/run/{{ NAME.stdout }}.pid
User={{ USER.stdout }}

[Install]
WantedBy=multi-user.target
